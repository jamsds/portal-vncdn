<div class="portal__main-center primary croped">
	<div class="dashboard">
		<div class="dashboard__container">
			<% if current_user.phone.nil? %>
			<div class="dashboard__container-message">
				dashboard.container.message
			</div>
			<% end %>
			<div class="dashboard__container-card clearfix">
				<div class="dashboard__container-card-item">
          <div class="form-group row">
            <label class="col-sm-2 col-form-label text-right">PropertyID</label>
            <div class="col-sm-8 col-form-value">
              <p><%= @response["id"] %></p>
              <small>Please provide PropertyID when you request support.</small>
            </div>
          </div>
          <div class="form-group row">
            <label class="col-sm-2 col-form-label text-right">Delivery Domain</label>
            <div class="col-sm-8 col-form-value">
              <p><%= @response["name"] %></p>
              <small>You should set a CNAME to delivery edge for web acceleration to work</small>
            </div>
          </div>
          <div class="form-group row">
            <label class="col-sm-2 col-form-label text-right">Origin</label>
            <div class="col-sm-8 col-form-value">
              <p><%= @response["originUrl"] %></p>
            </div>
          </div>
          <div class="form-group row">
            <label class="col-sm-2 col-form-label text-right">Streaming</label>
            <div class="col-sm-8 col-form-value">
              <p><% if @response["streamingService"] == true %>On<% else %>Off<% end %></p>
            </div>
          </div>
          <div class="form-group row">
            <label class="col-sm-2 col-form-label text-right">Delivery Edge</label>
            <div class="col-sm-8 col-form-value">
              <p><% if current_user.subscription.subscription_type == 1 %>vietnam.edge.vncdn.vn<% else %>global.edge.vncdn.vn<% end %></p>
            </div>
          </div>
          <% if params[:type] == "d" %>
          <div class="form-group row">
            <label class="col-sm-2 col-form-label text-right">WAF</label>
            <div class="col-sm-8 col-form-value">
              <p><% if @response["f5WafEnabled"] == false %>Disabled<% else %>Enabled<% end %></p>
            </div>
          </div>
          <% end %>
          <div class="form-group vertical row">
            <label class="col-sm-2 col-form-label text-right">Secret Token</label>
            <div class="col-sm-8 col-form-value">
              <input placeholder="Enter your name" class="form-control" type="text" value="<%= @response["tokenSecret"] %>" id="token" disabled="disabled">
            </div>
          </div>
        </div>
			</div>
		</div>
	</div>
</div>