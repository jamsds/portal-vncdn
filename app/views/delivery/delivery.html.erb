						</div>
					</div>
					<div class="main__options-list-item-info">
						<h6>Content Delivery Service</h6>
						<small>Manage your Web Acceleration Service</small> 
					</div>
					<div class="main__options-list-item-arrow">
						<% if current_page?(cdn_url) %>
						<div class="arrow_ic">
							<svg xmlns="http://www.w3.org/2000/svg" width="7.4" height="12" viewBox="0 0 7.4 12"><path d="M267.3,398.2l4.6-4.6-4.6-4.6,1.4-1.4,6,6-6,6Z" transform="translate(-267.3 -387.6)" fill="var(--main-color)"/></svg>
						</div>
						<% end %>
					</div>
				</a>
				<% if current_user.accountType == 2 %>
				<a href="<%= reseller_url %>" class="main__options-list-item clearfix<% if current_page?(reseller_url) %> active<% end %>">
					<div class="main__options-list-item-ic">
						<div class="reseller_ic">
							<svg xmlns="http://www.w3.org/2000/svg" width="18" height="20" viewBox="0 0 18 20"><path d="M277.7,1115.6a2.051,2.051,0,0,1,2,2v14a2.051,2.051,0,0,1-2,2h-14a1.816,1.816,0,0,1-1.4-.6,1.974,1.974,0,0,1-.6-1.4v-14a2.051,2.051,0,0,1,2-2h1v-2h2v2h8v-2h2v2Zm-1,15v-1c0-.9-.7-1.6-2.1-2.2a9.96,9.96,0,0,0-3.9-.9,9.587,9.587,0,0,0-3.9.9c-1.3.6-2.1,1.3-2.1,2.2v1Zm-3.9-11.1a2.9,2.9,0,1,0,.9,2.1A2.878,2.878,0,0,0,272.8,1119.5Z" transform="translate(-261.7 -1113.6)"/></svg>
						</div>
					</div>
					<div class="main__options-list-item-info">
						<h6>Reseller Management</h6>
						<small>Manage Reseller</small> 
					</div>
					<div class="main__options-list-item-arrow">
						<% if current_page?(reseller_url) %>
						<div class="arrow_ic">
							<svg xmlns="http://www.w3.org/2000/svg" width="7.4" height="12" viewBox="0 0 7.4 12"><path d="M267.3,398.2l4.6-4.6-4.6-4.6,1.4-1.4,6,6-6,6Z" transform="translate(-267.3 -387.6)" fill="var(--main-color)"/></svg>
						</div>
						<% end %>
					</div>
				</a>
				<% end %>
			</div>
		</div>
	</div>
</div>
<div class="portal__main-center croped">
	<div class="dashboard">
		<div class="dashboard__heading">
			<div class="dashboard__heading-action-list clearfix">
				<div class="dashboard__heading-action-item">
					<a href="<%= cdn_create_url %>" class="btn btn-sm ml-2">
						<span class="add_ic">
							<svg xmlns="http://www.w3.org/2000/svg" width="20.1" height="20" viewBox="0 0 20.1 20"><path d="M521.4,422.6v14h14v2h-14a2.051,2.051,0,0,1-2-2v-14Zm16.1-4a2.051,2.051,0,0,1,2,2v12a2.051,2.051,0,0,1-2,2h-12a2.051,2.051,0,0,1-2-2v-12a2.051,2.051,0,0,1,2-2Zm-1.1,9v-2h-4v-4h-2v4h-4v2h4v4h2v-4Z" transform="translate(-519.4 -418.6)"/></svg>
						</span>
						<span>Add Service</span>
					</a>
				</div>
				<div class="dashboard__heading-action-item">
					<a href="<%= request.fullpath %>" class="btn btn-sm ml-2">
						<span class="reload_ic">
							<svg xmlns="http://www.w3.org/2000/svg" width="16" height="20" viewBox="0 0 16 20"><path d="M455.5,38.8a8.1,8.1,0,0,1,5.7,2.3,7.473,7.473,0,0,1,2.3,5.6,7.787,7.787,0,0,1-2.4,5.7,7.626,7.626,0,0,1-5.6,2.3,8.023,8.023,0,0,1-8-8h2a5.447,5.447,0,0,0,1.8,4.2,6.174,6.174,0,0,0,4.2,1.8,5.6,5.6,0,0,0,4.2-1.8,6.174,6.174,0,0,0,1.8-4.2,5.447,5.447,0,0,0-1.8-4.2,6.174,6.174,0,0,0-4.2-1.8v4l-5-5,5-5Z" transform="translate(-447.5 -34.7)"/></svg>
						</span>
						<span>Reload</span>
					</a>
				</div>
				<div class="dashboard__heading-action-item">
					<div class="btn btn-sm ml-2 disabled">
						<span class="pause_ic">
							<svg xmlns="http://www.w3.org/2000/svg" width="12" height="14" viewBox="0 0 12 14"><path d="M856.1,18V4h4V18Zm8-14h4V18h-4Z" transform="translate(-856.1 -4)"/></svg>
						</span>
						<span>Stop</span>
					</div>
				</div>
				<div class="dashboard__heading-action-item">
					<div class="btn btn-sm ml-2 disabled">
						<span class="trash_ic">
							<svg xmlns="http://www.w3.org/2000/svg" width="14" height="18" viewBox="0 0 14 18"><path d="M425.5,1047v2h-14v-2H415l1-1h5l1,1Zm-12.9,15v-12h12v12a2.051,2.051,0,0,1-2,2h-8a2.051,2.051,0,0,1-2-2Z" transform="translate(-411.5 -1046)"/></svg>
						</span>
						<span>Delete</span>
					</div>
				</div>
			</div>
		</div>
		<div class="dashboard__container">
			<% if current_user.phone.nil? %>
			<div class="dashboard__container-message hidden">
				dashboard.container.message
			</div>
			<% end %>
			<div class="dashboard__container-card clearfix">
				<div class="dashboard__container-card">
					<div class="dashboard__container-card-item">
						<div class="delivery">
							<div class="dashboard__container-table-search mb-3">
								<input type="" name="" id="search" class="dashboard__container-table-search-input" placeholder="Filter Domain">
							</div>
							<div class="table-responsive">
								<table class="table croped mb-0">
									<thead>
										<tr>
											<th>Domain</th>
											<th>Scenario Type</th>
											<th>Operation</th>
										</tr>
									</thead>
									<tbody>
										<% @domainItems.each do |item| %>
											<% if !item["f5WafEnabled"].nil? %>
												<tr>
													<td><% if item["active"] == true %><span class="running_ic mr-2"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 20 20"><path d="M115.8,38.8a9.506,9.506,0,0,1,7.1-2.9,9.9,9.9,0,0,1,7.1,2.9,9.506,9.506,0,0,1,2.9,7.1A9.9,9.9,0,0,1,130,53a9.506,9.506,0,0,1-7.1,2.9,9.9,9.9,0,0,1-7.1-2.9,9.506,9.506,0,0,1-2.9-7.1A9.732,9.732,0,0,1,115.8,38.8Zm5.1,11.5,6-4.5-6-4.5Z" transform="translate(-112.9 -35.9)" fill="#41c300"/></svg></span><% else %><span class="stopped_ic mr-2"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 20 20"><path d="M4.9,38.8A9.506,9.506,0,0,1,12,35.9a10.068,10.068,0,0,1,7.1,2.9A9.506,9.506,0,0,1,22,45.9,9.9,9.9,0,0,1,19.1,53,9.506,9.506,0,0,1,12,55.9,9.9,9.9,0,0,1,4.9,53,9.506,9.506,0,0,1,2,45.9,9.732,9.732,0,0,1,4.9,38.8Zm6.1,11v-8H9v8Zm4,0v-8H13v8Z" transform="translate(-2 -35.9)" fill="#B1B1B1"/></svg></span><% end %><a href="<%= cdn_url %>/<%= item["id"] %>/d"><%= item["name"] %></a></td>
													<td><span>Website Acceleration</span></td>
													<td><a href="<%= cdn_url %>/<%= item["id"] %>/d/edit" class="btn btn-default primary outline btn-sm">Edit</a></td>
												</tr>
											<% else %>
												<tr>
													<td><% if item["active"] == true %><span class="running_ic mr-2"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 20 20"><path d="M115.8,38.8a9.506,9.506,0,0,1,7.1-2.9,9.9,9.9,0,0,1,7.1,2.9,9.506,9.506,0,0,1,2.9,7.1A9.9,9.9,0,0,1,130,53a9.506,9.506,0,0,1-7.1,2.9,9.9,9.9,0,0,1-7.1-2.9,9.506,9.506,0,0,1-2.9-7.1A9.732,9.732,0,0,1,115.8,38.8Zm5.1,11.5,6-4.5-6-4.5Z" transform="translate(-112.9 -35.9)" fill="#41c300"/></svg></span><% else %><span class="stopped_ic mr-2"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 20 20"><path d="M4.9,38.8A9.506,9.506,0,0,1,12,35.9a10.068,10.068,0,0,1,7.1,2.9A9.506,9.506,0,0,1,22,45.9,9.9,9.9,0,0,1,19.1,53,9.506,9.506,0,0,1,12,55.9,9.9,9.9,0,0,1,4.9,53,9.506,9.506,0,0,1,2,45.9,9.732,9.732,0,0,1,4.9,38.8Zm6.1,11v-8H9v8Zm4,0v-8H13v8Z" transform="translate(-2 -35.9)" fill="#B1B1B1"/></svg></span><% end %><a href="<%= cdn_url %>/<%= item["id"] %>/f"><%= item["name"] %></a></td>
													<td><span>Large File Download</span></td>
													<td>
														<a href="<%= cdn_url %>/<%= item["id"] %>/f/edit" class="btn btn-default primary outline btn-sm">Edit</a>
														<% if item["active"] == true %>
															<a class="btn btn-default warning outline btn-sm ml-2" data-method="post" href="/cdn/<%= item["id"] %>/f/stop?streamingService=<%= item["streamingService"] %>">Disable</a>
														<% else %>
															<a class="btn btn-default primary outline btn-sm ml-2" data-method="post" href="/cdn/<%= item["id"] %>/f/start?streamingService=<%= item["streamingService"] %>">Start</a>
														<% end %>
													</td>
												</tr>
											<% end %>
										<% end %>
									</tbody>
								</table>
							</div>
							<%= will_paginate @domainItems %>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>