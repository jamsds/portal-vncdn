<div class="portal__main-heading">
	<span class="btn btn-default btn-sm ml-2">
		<a href="<%= url_for(:back) %>" class="back_ic"><svg xmlns="http://www.w3.org/2000/svg" width="18" height="12" viewBox="0 0 18 12"><path d="M353.6,392.8v2H339.4l3.6,3.6-1.4,1.4-6-6,6-6,1.4,1.4-3.6,3.6Z" transform="translate(-335.6 -387.8)" fill="#f68100"/></svg></a>
	</span>
</div>
<div class="portal__main-left expanded">
  <div class="panel">
    <div class="panel__list main">
      <div class="main__options-list">
        <div class="main__options-list-item clearfix<% if current_page?(cdn_url) %> active<% end %>" onclick="window.location = '/cdn/<%= params[:propertyId] %>/<%= params[:type] %>/reports?domain=<%= params[:domain] %>'">
          <div class="main__options-list-item-ic">
            <div class="was_ic">
              <svg xmlns="http://www.w3.org/2000/svg" width="20.9" height="18" viewBox="0 0 20.9 18"><path d="M856.7,1118.2a8.6,8.6,0,0,1,6.4-2.6,9,9,0,0,1,6.4,2.6,8.448,8.448,0,0,1,2.6,6.4,9,9,0,0,1-2.6,6.4,8.448,8.448,0,0,1-6.4,2.6,8.6,8.6,0,0,1-6.3-2.6l1.4-1.5a6.662,6.662,0,0,0,4.9,2.1,7.2,7.2,0,0,0,5-2,6.767,6.767,0,0,0,0-9.8,7.028,7.028,0,0,0-5-2,6.632,6.632,0,0,0-4.9,2,6.8,6.8,0,0,0-2,4.9h3l-4,4-.1-.1-3.9-3.9h3A8.232,8.232,0,0,1,856.7,1118.2Zm5.4,2.4h1.5v4.2l3.5,2.1-.8,1.2-4.3-2.6v-4.9Z" transform="translate(-851.2 -1115.6)"/></svg>
            </div>
          </div>
          <div class="main__options-list-item-info">
            <h6>Logs</h6>
            <small>Download Log files of this service</small>
          </div>
          <div class="main__options-list-item-arrow">
            <% if current_page?(cdn_url) %>
            <div class="arrow_ic">
              <svg xmlns="http://www.w3.org/2000/svg" width="7.4" height="12" viewBox="0 0 7.4 12"><path d="M267.3,398.2l4.6-4.6-4.6-4.6,1.4-1.4,6,6-6,6Z" transform="translate(-267.3 -387.6)" fill="#f68100"/></svg>
            </div>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="portal__main-center primary croped">
	<div class="dashboard">
		<div class="dashboard__container">
			<% if current_user.phone.nil? %>
			<div class="dashboard__container-message">
				dashboard.container.message
			</div>
			<% end %>
			<div class="dashboard__container-card clearfix">
				<div class="dashboard__container-table">
					<div class="dashboard__container-table-select clearfix">
						<a href="<%= cdn_url %>/<%= params[:propertyId] %>/<%= params[:type] %>" class="dashboard__container-table-select-item">Details</a>
						<a href="<%= cdn_url %>/<%= params[:propertyId] %>/<%= params[:type] %>/reports?domain=<%= params[:domain] %>" class="dashboard__container-table-select-item">Reports</a>
						<a href="<%= request.fullpath %>" class="dashboard__container-table-select-item active">Logs</a>
					</div>
					<div class="dashboard__container-table-content mt-3">
						<div class="dashboard__container-card mt-3">
							<div class="dashboard__container-card-item">
								<table class="table mb-0">
									<thead>
										<tr>
											<th>Log File Name</th>
											<th>Size</th>
											<th>Log Date</th>
											<th>Actions</th>
										</tr>
									</thead>
									<tbody>
										<% @logItems.each do |time, item| %>
											<tr>
												<td><%= item["name"] %></td>
												<td><%= number_to_human_size(item["size"], separator: ".", delimiter: ",") %></td>
												<td><%= time %></td>
												<td><a href="<%= item["url"] %>" class="primary">Download</a></td>
											</tr>
										<% end %>
									</tbody>
								</table>
								<%= will_paginate @logItems, :params => {:for => params[:domain]} %>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>